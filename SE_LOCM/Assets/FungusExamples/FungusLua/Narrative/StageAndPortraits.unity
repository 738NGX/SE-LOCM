  m_Name: 
  m_EditorClassIdentifier: 
  itemId: 4
  indentLevel: 0
  luaEnvironment: {fileID: 0}
  luaFile: {fileID: 0}
  luaScript: '-- Shows how to use stage.show with positional arguments

    -- This
    method is less verbose but also easier to get wrong!


    stage.show(watson,
    "left")

    setcharacter(watson)

    say "Sherlock....."


    stage.show(sherlock,
    "angry", "offscreen right", "right")

    setcharacter(sherlock)

    say "{i}what?{/i}"


    setcharacter(watson)

    say
    "You don''t understand how I feel! I''m leaving!"

    stage.hide(watson, "offscreen
    left")


    stage.showPortrait(sherlock, "bored")

    setcharacter(sherlock)

    say
    "Look at me~! I''m Watson~! I''m grumpy all the tii~me."

    stage.hide(sherlock,
    "offscreen right")


    stage.show(watson, "confident", "offscreen left", "left")

    setcharacter(watson)

    say
    "I showed him!"

    stage.hide(watson)'
  runAsCoroutine: 1
  waitUntilFinished: 1
  returnVariable: {fileID: 0}
